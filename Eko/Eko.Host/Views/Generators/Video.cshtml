    <!-- Dashboard page wrapper start -->
    <div class="page">
        <header>
            @await Html.PartialAsync("_HeaderDashboard")
        </header>

        <!-- Start app-content-area -->
        <div class="app-content-area p-relative">
            <div class="app-content-wrap app-gap-bottom">
                <div class="generator-box-wrapper generator-gap">
                    <div class="single-item-generator">
                        <div class="chat-box">
                            <div class="chat-box-author d-flex flex-wrap justify-end align-start gap-20">
                                <div class="author-text-box d-flex align-start gap-15">
                                    <div class="d-flex align-center gap-10">
                                        <button type="button" class="author-text-btn bd-tooltip-show">
                                            <i class="ri-file-copy-line"></i>
                                            <span class="bd-tooltip">Copy</span>
                                        </button>
                                        <button type="button" class="author-text-btn bd-tooltip-show">
                                            <i class="ri-edit-fill"></i>
                                            <span class="bd-tooltip">Edit message</span>
                                        </button>
                                        <button type="button" class="author-text-btn bd-tooltip-show">
                                            <i class="ri-delete-bin-line"></i>
                                            <span class="bd-tooltip">Delete</span>
                                        </button>
                                    </div>
                                    <div class="text">
                                        Generate a promotional video for our latest product.
                                        <div class="text-thumb">
                                            <img src="../images/generator/generator-video-001.webp" alt="">
                                        </div>
                                    </div>
                                </div>
                                <div class="author">
                                    <div class="author-thumb">
                                        <img src="../images/avatar/main-user.webp" alt="user">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="chat-box">
                            <div class="d-flex flex-wrap justify-start align-start gap-20">
                                <div class="ekoai-thumb"><img src="../images/logo/favicon.svg" alt=""></div>
                                <div class="ekoai-body">
                                    <div class="prompt-box mb-30">
                                        <div class="head d-flex justify-between">
                                            <div class="">prompt</div>
                                            <div class="meta-btn d-flex align-center gap-15">
                                                <button type="button" class="meta-btn-icon bd-tooltip-show">
                                                    <i class="ri-file-copy-line"></i> Copy
                                                    <span class="bd-tooltip">Copy</span>
                                                </button>

                                                <button type="button" class="meta-btn-icon bd-tooltip-show">
                                                    <i class="ri-pencil-line"></i> Edit
                                                    <span class="bd-tooltip">Edit in canvas</span>
                                                </button>
                                            </div>
                                        </div>
                                        <div class="content">Create a 30-second promotional video featuring our latest
                                            product. The video should highlight the product's key features, benefits,
                                            and applications. Use a modern style with smooth transitions, upbeat
                                            background music, and on-screen text for better engagement. Output should be
                                            in 1080p with dynamic lighting effects and brand colors incorporated.
                                        </div>
                                    </div>
                                    <div class="generator-image-wrap">
                                        <h6 class="title fs-16">Generated Video Preview:</h6>
                                        <div class="generator-image mb-25">
                                            <div class="generator-image-btn d-flex align-center justify-between">
                                                <div class="like-dislike-btn d-flex align-center gap-05">
                                                    <button class="generator-btn" type="button"><i
                                                            class="ri-thumb-up-line"></i></button>
                                                    <button class="generator-btn dislike" type="button"><i
                                                            class="ri-thumb-down-line"></i></button>
                                                </div>
                                                <div class="download-btn">
                                                    <button class="generator-btn download" type="button"><i
                                                            class="ri-download-line"></i></button>
                                                </div>
                                            </div>
                                            <div class="generator-video"><video loop="" controls="" src="../video/generat-video-002.mp4"></video></div>
                                            <div class="generator-image-btn bottom">
                                                <button class="generator-btn w-auto" type="button">
                                                    <i class="ri-repeat-line"></i> Regenerate</button>
                                            </div>
                                            <div class="generator-image-btn bottom">
                                                <button class="generator-btn w-auto" type="button">
                                                    <i class="ri-repeat-line"></i> Regenerate</button>
                                            </div>
                                        </div>

                                        <div class="generator-info">
                                            <h6 class="title">Select the modification number:</h6>
                                            <ul class="generator-info-list sl-number-counts">
                                                <li> Add product-specific animations for better engagement.</li>
                                                <li> Use a voiceover to explain the key features and benefits.</li>
                                                <li> Adjust the background music to be more dynamic and energetic.</li>
                                            </ul>
                                            <p class="description">Let us know if youâ€™d like further adjustments to this
                                                video!</p>
                                            <div class="generator-info-btn d-flex align-center gap-05">
                                                <button class="generator-btn bd-tooltip-show" type="button">
                                                    <i class="ri-volume-up-line"></i>
                                                    <span class="bd-tooltip bottom">Read aloud</span>
                                                </button>
                                                <button class="generator-btn bd-tooltip-show" type="button">
                                                    <i class="ri-thumb-up-line"></i>
                                                    <span class="bd-tooltip bottom">Good response</span>
                                                </button>
                                                <button class="generator-btn bd-tooltip-show" type="button">
                                                    <i class="ri-thumb-down-line"></i>
                                                    <span class="bd-tooltip bottom">bad response</span>
                                                </button>
                                                <button class="generator-btn bd-tooltip-show" type="button">
                                                    <i class="ri-edit-box-line"></i>
                                                    <span class="bd-tooltip bottom">Edit in canvas</span>
                                                </button>
                                                <button class="generator-btn bd-tooltip-show" type="button">
                                                    <i class="ri-share-line"></i>
                                                    <span class="bd-tooltip bottom">Share</span>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
