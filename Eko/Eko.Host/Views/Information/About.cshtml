<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>About</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Place favicon.ico in the root directory -->
    <link rel="shortcut icon" type="image/x-icon" href="../images/logo/favicon.svg">
    <!-- CSS here -->
    <link rel="stylesheet" href="../css/vendor/bootstrap.min.css">
    <link rel="stylesheet" href="../css/vendor/animate.min.css">
    <link rel="stylesheet" href="../css/plugins/swiper.min.css">
    <link rel="stylesheet" href="../css/vendor/magnific-popup.css">
    <link rel="stylesheet" href="../css/vendor/icomoon.css">
    <link rel="stylesheet" href="../css/vendor/remixicon.css">
    <link rel="stylesheet" href="../css/vendor/spacing.css">
    <link rel="stylesheet" href="../css/main.css">
</head>

<body>
<header>
    @await Html.PartialAsync("_Header")
</header>


    <!-- Body main wrapper start -->
    <main>

        <!-- breadcrumb area start -->
        <section class="bd-breadcrumb-area">
            <div class="container">
                <div class="row justify-center">
                    <div class="col-xl-6 col-lg-6 col-md-8">
                        <div class="bd-breadcrumb-wrapper">
                            <div class="bd-breadcrumb-content text-center">
                                <h1 class="bd-breadcrumb-title mb-20">About</h1>
                                <div class="bd-breadcrumb-list">
                                    <span><i class="ri-home-4-line"></i> EkoAI</span>
                                    <span class="divider"><i class="ri-arrow-right-s-line"></i></span>
                                    <span class="active">About Us</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="bd-breadcrumb-shape">
                <img src="../images/shape/breadcrumb-shape.webp" alt="">
                <div class="bd-breadcrumb-glow"></div>
            </div>
        </section>
        <!-- breadcrumb area end -->

        <!-- about area start -->
        <div class="bd-about-area section-gap">
            <div class="container">
                <div class="row gy-30 justify-center wow bdFadeInUp" data-wow-duration="1s" data-wow-delay=".2s">
                    <div class="col-xl-10 col-lg-10 col-md-12">
                        <div class="bd-about-wrapper text-center">
                            <div class="bd-about-thumb mb-30">
                                <img src="../images/about/about-image-005.webp" alt="about">
                            </div>
                            <div class="bd-about-intro">We are building safe and beneficial AGI, but will also consider our
                                mission
                                fulfilled if our work aids others to achieve this outcome.</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- about area end -->

        <!-- mission-vision-values area start -->
        <section class="bd-mvv-area section-gap fix section-separator">
            <div class="section-glow-wrapper">
                <div class="section-glow"></div>
                <div class="section-glow glow-2"></div>
            </div>
            <div class="container">
                <div class="row justify-center wow bdFadeInUp" data-wow-duration="1s" data-wow-delay=".2s">
                    <div class="col-xl-6">
                        <div class="bd-section-title-wrapper section-title-gap text-center">
                            <h2 class="bd-section-title">Empowering Creativity Through AI</h2>
                            <p class="bd-section-intro">
                                We democratize AI-driven content creation, empowering everyone to create effortlessly with
                                innovation and inclusivity.
                            </p>
                        </div>
                    </div>
                </div>
                <div class="row gy-30 justify-center wow bdFadeInUp" data-wow-duration="1.5s" data-wow-delay=".2s">
                    <!-- Vision Section -->
                    <div class="col-xl-4 col-lg-4 col-md-6">
                        <div class="bd-mvv-box-item light-effect">
                            <div class="bd-mvv-box-icon"><img src="../images/icon/vision-icon.webp" alt="vision-icon"></div>
                            <h4 class="title">OUR VISION</h4>
                            <p class="intro">
                                To empower creativity by delivering cutting-edge AI tools that simplify content creation for
                                everyone—from individuals to enterprises—unlocking limitless possibilities.
                            </p>
                        </div>
                    </div>

                    <!-- Mission Section -->
                    <div class="col-xl-4 col-lg-4 col-md-6">
                        <div class="bd-mvv-box-item light-effect">
                            <div class="bd-mvv-box-icon"><img src="../images/icon/mission-icon.webp" alt="mission-icon">
                            </div>
                            <h4 class="title">OUR MISSION</h4>
                            <p class="intro">
                                To democratize AI-driven content creation, making it accessible, efficient, and intuitive so
                                users can effortlessly generate stunning visuals and impactful content.
                            </p>
                        </div>
                    </div>

                    <!-- Values Section -->
                    <div class="col-xl-4 col-lg-4 col-md-6">
                        <div class="bd-mvv-box-item light-effect">
                            <div class="bd-mvv-box-icon"><img src="../images/icon/value-icon.webp" alt="value-icon"></div>
                            <h4 class="title">OUR VALUES</h4>
                            <p class="intro">
                                Innovation, inclusivity, and excellence drive us. We believe in creating tools that inspire
                                creativity, foster collaboration, and deliver exceptional results for every user.
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- mission-vision-values area end -->

        <!-- video area start -->
        <div class="bd-video-area p-relative section-gap">
            <div class="bd-video-bg include-bg bg-attach-fix" data-background="../images/bg/video-bg-1.webp">
            </div>
            <div class="container">
                <div class="row align-center justify-center">
                    <div class="col-xl-6 col-lg-4">
                        <div class="bd-video-play-btn">
                            <a class="bd-video-btn popup-video pos-center" href="https://www.youtube.com/watch?v=haHjGehC1dg">
                                <span class="icon"><i class="ri-play-large-fill"></i></span>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- cta area start -->
        <section class="bd-cta-wrapper p-relative section-gap section-separator-top">
            <div class="bd-cta-glow d-none d-sm-block">
                <div class="glow-1"></div>
                <div class="glow-2"></div>
            </div>
        </section>
    </main>
<footer class="bd-footer-area">
    <div class="bd-footer-copyright-area pt-25 pb-20">
        <div class="container">
            <div class="row gy-30 align-center justify-content-lg-between justify-center">
                <div class="col-xxl-4 col-xl-5 col-lg-5 col-md-8">
                    <div class="bd-footer-copyright text-lg-start text-center">
                        <p class="underline">© Copyright <span id="year"></span> Developed By <span class="c-name">EkoAI</span></p>
                    </div>
                </div>
                <div class="col-xxl-5 col-xl-7 col-lg-7 col-md-12">
                    <div class="bd-footer-copyright-list">
                        <ul class=" d-flex justify-content-lg-end justify-center">
                            <li class="underline"><a href="/Dashboard/TermsConditions">Terms and Conditions</a></li>
                            <li class="underline"><a href="/Dashboard/PrivacyPolicy">Privacy and Policy</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</footer>
    <!-- footer area end -->

    <!-- back to top area start -->
    <div class="progress-wrap">
        <svg class="progress-circle svg-content" width="100%" height="100%" viewBox="-1 -1 102 102">
            <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" />
        </svg>
    </div>
    <!-- back to top area end -->

    <!-- switcher area start -->
    <div class="theme-switcher">
        <i class="ri-sun-line change-theme" id="theme-button"></i>
    </div>
    <!-- switcher area start -->

    <!-- JS here -->
    <script data-cfasync="false" src="../cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script src="../js/vendor/jquery-3.7.1.min.js"></script>
    <script src="../js/plugins/waypoints.min.js"></script>
    <script src="../js/vendor/bootstrap.bundle.min.js"></script>
    <script src="../js/plugins/swiper.min.js"></script>
    <script src="../js/plugins/wow.js"></script>
    <script src="../js/vendor/magnific-popup.min.js"></script>
    <script src="../js/vendor/isotope.pkgd.min.js"></script>
    <script src="../js/vendor/imagesloaded.pkgd.min.js"></script>
    <script src="../js/plugins/nice-select.min.js"></script>
    <script src="../js/vendor/ajax-form.js"></script>
    <script src="../js/plugins/easypie.js"></script>
    <script src="../js/vendor/purecounter.js"></script>
    <script src="../js/vendor/backtotop.js"></script>
    <script src="../js/plugins/prism.js"></script>
    <script src="../js/plugins/typed.js"></script>
    <script src="../js/main.js"></script>
<script type="module" src="/js/forALL.js"></script>
</body>

</html>